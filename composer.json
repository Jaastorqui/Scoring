{
    "name": "clickhouse-admin-panel/poc",
    "description": "ClickHouse Admin Panel PoC - Business Scores Analytics",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-curl": "*",
        "ext-json": "*",
        "smi2/phpclickhouse": "^1.6",
        "symfony/http-client": "^7.3",
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "scripts": {
        "generate": "php app/generate.php"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
